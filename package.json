{
  "name": "dzydo-club",
  "version": "0.1.0",
  "description": "DzydoClub TypeScript starter project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest --runInBand",
    "prisma:generate": "prisma generate",
    "db:init": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "ts-node src/seed.ts",
    "setup": "npm run prisma:generate && npm run db:migrate && npm run db:seed"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^17.2.4",
    "jest": "^29.5.0",
    "prisma": "^4.15.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
